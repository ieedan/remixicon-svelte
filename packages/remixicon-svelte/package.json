{
	"name": "remixicon-svelte",
	"description": "RemixIcon for Svelte",
	"version": "0.0.2",
	"keywords": [
		"svelte, remixicon"
	],
	"license": "MIT",
	"author": {
		"name": "Aidan Bleser",
		"url": "https://github.com/ieedan"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ieedan/remixicon-svelte"
	},
	"bugs": {
		"url": "https://github.com/ieedan/remixicon-svelte/issues"
	},
	"scripts": {
		"dev": "pnpm watch",
		"build": "pnpm prepack",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"watch": "svelte-package --watch"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./icons/*.svelte": {
			"types": "./dist/icons/*.svelte.d.ts",
			"svelte": "./dist/icons/*.svelte"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.50.1",
		"@sveltejs/package": "^2.5.7",
		"@types/node": "^22",
		"globals": "^17.1.0",
		"publint": "^0.3.17",
		"svelte": "^5.48.2",
		"svelte-check": "^4.3.5",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"vite": "^7.3.1"
	}
}
